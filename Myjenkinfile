pipeline {
  agent any;
  stages {
    stage ('BUILD') {
      parallel {
      steps {
        echo "This is Build stage in chrome"
        sh 'sleep 5'
      }
      steps {
        echo "This is Build stage in Edge"
        sh 'sleep 5'
      }
      steps {
        echo "This is Build stage in Firefox"
        sh 'sleep 5'
      }
    }
    }
    stage ('DEPLOY') {
      steps {
        echo "This is Deploy stage"
      }
    }
    stage ('TEST') {
      steps {
        echo "This is Test stage"
      }
    }
  }
}
